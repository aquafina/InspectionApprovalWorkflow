<process name="BPEL_SetApprovedStatus"
         targetNamespace="http://xmlns.oracle.com/InspectionApprovalWorkflow/InspectionApproval/BPEL_SetApprovedStatus"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:client="http://xmlns.oracle.com/InspectionApprovalWorkflow/InspectionApproval/BPEL_SetApprovedStatus"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/InspectionApprovalWorkflow/InspectionApproval/SetApprovedStatus"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="bpel_setapprovedstatus_client" partnerLinkType="client:BPEL_SetApprovedStatus" myRole="BPEL_SetApprovedStatusProvider" partnerRole="BPEL_SetApprovedStatusRequester"/>
        <partnerLink name="SetApprovedStatus"
                     partnerLinkType="ns1:SetApprovedStatus_plt"
                     partnerRole="SetApprovedStatus_role"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable" messageType="client:BPEL_SetApprovedStatusRequestMessage"/>

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable" messageType="client:BPEL_SetApprovedStatusResponseMessage"/>
        <variable name="Invoke_SetApprovedStatus_merge_InputVariable"
                  messageType="ns1:RcvTransactionsCollection_msg"/>
    </variables>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in BPEL_SetApprovedStatus.wsdl) -->
        <receive name="receiveInput" partnerLink="bpel_setapprovedstatus_client" portType="client:BPEL_SetApprovedStatus" operation="process" variable="inputVariable" createInstance="yes"/>
        <assign name="Transform1">
            <bpelx:annotation>
                <bpelx:pattern>transformation</bpelx:pattern>
            </bpelx:annotation>
            <copy>
                <from expression="ora:doXSLTransformForDoc('xsl/Transformation_2.xsl', $inputVariable.payload)"/>
                <to variable="Invoke_SetApprovedStatus_merge_InputVariable"
                    part="RcvTransactionsCollection"/>
            </copy>
        </assign>
        <invoke name="Invoke_SetApprovedStatus"
                inputVariable="Invoke_SetApprovedStatus_merge_InputVariable"
                partnerLink="SetApprovedStatus"
                portType="ns1:SetApprovedStatus_ptt" operation="merge"
                bpelx:invokeAsDetail="no"/>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <invoke name="callbackClient" partnerLink="bpel_setapprovedstatus_client" portType="client:BPEL_SetApprovedStatusCallback" operation="processResponse" inputVariable="outputVariable"/>
    </sequence>
</process>